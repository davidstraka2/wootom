    "lint:check": "eslint src --max-warnings=0",
    "lint:fix": "eslint src --max-warnings=0 --fix",
    "pack": "node scripts/pack.mjs"
  },
  "devDependencies": {
    "@types/atom": "^1.40.7",
    "@types/jasmine": "^3.6.4",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^8.1.0",
    "fs-extra": "^9.1.0",
    "prettier": "2.2.1",
    "typescript": "^4.1.5"
  }
