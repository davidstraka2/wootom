  "license": "MIT",
  "homepage": "https://github.com/davidstraka2/wootom#readme",
  "description": "WooWoo Editor for Atom",
  "keywords": [
    "WooWoo",
    "woo"
  ],
  "bugs": {
    "url": "https://github.com/davidstraka2/wootom/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/davidstraka2/wootom.git"
  },
  "main": "./lib/wootom",
  "engines": {
    "atom": "^1.54.0"
  },
  "activationCommands": {
